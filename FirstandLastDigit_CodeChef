def lastdigit(number):
    number = number %10
    return number
def firstdigit(number):
    while(number>=10):
        number = number//10
    return number

n = int(input())
for i in range(n):
    a = int(input())
    first = firstdigit(a)
    last = lastdigit(a)
    sum = first + last
    print(sum)
